{
  "name": "@blablacar/ui-library",
  "version": "13.0.1",
  "description": "BlaBlaCar React UI component library",
  "main": "build/index.js",
  "homepage": "https://blablacar.github.io/ui-library",
  "repository": {
    "type": "git",
    "url": "git@github.com:blablacar/ui-library.git"
  },
  "scripts": {
    "start": "export NODE_PATH=src && start-storybook",
    "build": "rimraf build && tsc && babel build/src -x \".jsx\",\".js\" --ignore \"**/*.unit.jsx\",\"**/story.jsx\" --out-dir build && rimraf build/src && rimraf build/stories",
    "tslint": "tslint \"src/**/*.ts\" \"src/**/*.tsx\" --project tsconfig.json",
    "lint": "git diff --name-only --cached --diff-filter=ACMRTUXB -- \"src/*.ts\" \"src/*.tsx\" | xargs tslint",
    "prettier": "prettier --write '**/*.{ts,tsx}'",
    "prettier:ci": "prettier --list-different '**/*.{ts,tsx}'",
    "test": "jest",
    "test:coverage": "jest --no-cache --coverage",
    "watch": "npm test -- --watch --onlyChanged",
    "bla": "bla",
    "prepublishOnly": "npm run tslint && npm run test && npm run build",
    "storybook-build": "export NODE_PATH=src && build-storybook -c .storybook -o storybook-build",
    "storybook-deploy": "npm run storybook-build && gh-pages -d storybook-build"
  },
  "author": "Blablacar front-end team",
  "keywords": [
    "blablacar",
    "design-system",
    "atomic-design",
    "ui-library",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-transform-react-jsx": "7.3.0",
    "@babel/preset-env": "7.4.1",
    "@babel/preset-react": "7.0.0",
    "@blablacar/cli": "1.2.0",
    "@storybook/addon-a11y": "5.0.3",
    "@storybook/addon-actions": "5.0.3",
    "@storybook/addon-knobs": "5.0.3",
    "@storybook/addon-options": "5.0.3",
    "@storybook/addon-viewport": "5.0.3",
    "@storybook/addons": "5.0.3",
    "@storybook/react": "5.0.3",
    "@types/enzyme": "3.9.1",
    "@types/exenv": "1.2.0",
    "@types/jest": "24.0.15",
    "@types/lodash.debounce": "4.0.6",
    "@types/lodash.isempty": "4.4.6",
    "@types/lodash.isequal": "4.5.5",
    "@types/lodash.isstring": "4.0.6",
    "@types/react": "16.8.8",
    "@types/react-color": "3.0.1",
    "@types/react-dom": "16.8.2",
    "@types/react-test-renderer": "16.8.1",
    "@types/react-transition-group": "2.0.16",
    "@types/storybook__react": "4.0.2",
    "@types/styled-components": "4.1.8",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-typescript-to-proptypes": "0.17.1",
    "caniuse-db": "1.0.30000951",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "gh-pages": "2.0.1",
    "husky": "1.3.1",
    "jest": "24.8.0",
    "jest-styled-components": "6.3.3",
    "jsdom": "14.0.0",
    "jsdom-global": "3.0.2",
    "prettier": "1.16.4",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-test-renderer": "16.9.0",
    "rimraf": "2.6.3",
    "storybook-readme": "5.0.8",
    "styled-components": "4.3.2",
    "tslint": "5.14.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.6.3"
  },
  "dependencies": {
    "classcat": "3.2.5",
    "country-telephone-data": "0.6.0",
    "exenv": "1.2.2",
    "focus-trap": "5.0.0",
    "lodash.debounce": "4.0.8",
    "lodash.isempty": "4.4.0",
    "lodash.isequal": "4.5.0",
    "lodash.isstring": "4.0.1",
    "react-day-picker": "7.3.0",
    "react-transition-group": "2.6.1"
  },
  "peerDependencies": {
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "styled-components": "4.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "bla node && bla check && tsc --noEmit && npm run lint",
      "pre-push": "npm run tslint && npm test"
    }
  }
}
